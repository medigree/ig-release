        },
        "min": 1,
        "type": [
          {
            "code": "code"
          }
        ]
      },
      {
        "id": "Extension.extension:role.valueCode",
        "path": "Extension.extension.valueCode",
        "min": 1,
        "max": "1",
        "example": [
          {
            "valueCode": "MTH",
            "label": "Valid Example"
          }
